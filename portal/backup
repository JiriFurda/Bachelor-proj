curl -X PUT "localhost:9200/xstane34_projects/_mapping/data" -H 'Content-Type: application/json' -d'
{
  "properties": {
    "fundedUnder.programme": { 
      "type":     "text",
      "fielddata": true
    }
  }
}
'

